{
  "compilerOptions": {
    "baseUrl": "./src",
    "paths": {
      "@store": ["./app/store"],
      "@components/*": ["./components/*"],
      "@features/*": ["./features/*"],
      "@editorSlice/*": ["./features/editorSlice"],
      "@constants/*": ["./constants/*"],
      "@hooks/*": ["./hooks/*"],
      "@i18n/*": ["./i18n/*"],
      "@lib/*": ["./lib/*"],
      "@axios/*": ["./lib/axios/*"],
      "@customTypes/*": ["./types/*"],
      "@data/*": ["./mocks/data/*"],
      "@browser": ["./mocks/browser"],
      "@server": ["./mocks/server"],
      "@services/*": ["./services/*"],
      "@mocks/*": ["./mocks/*"],
      "@utils/*": ["./utils/*"],
      "@node_modules/*": ["./node_modules/*"],
      "@images/*": ["./assets/images/*"]
    },
    "target": "es6",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx"
  },
  "include": ["src"]
}
